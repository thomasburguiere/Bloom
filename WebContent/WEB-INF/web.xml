<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<welcome-file-list>
		<welcome-file>HomePage.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>Controler</servlet-name>
		<servlet-class>src.servlets.Controler</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Controler</servlet-name>
		<url-pattern>/controler</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Initialise</servlet-name>
		<servlet-class>src.beans.Initialise</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Initialise</servlet-name>
		<url-pattern>/initialise</url-pattern>
	</servlet-mapping>
</web-app>